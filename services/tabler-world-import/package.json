  "private": true,

  "dependencies": {
    "@mskg/tabler-world-aws": "0.0.1",
    "@mskg/tabler-world-cache": "0.0.1",
    "@mskg/tabler-world-rds-client": "0.0.1",
    "@sentry/node": "^5.4.0",
    "aws-xray-sdk": "^2.3.3",
    "https": "^1.0.0",
    "lodash": "^4.17.11",
    "node-geocoder": "^3.23.0",
    "node-gzip": "^1.1.2",
    "pg": "^7.8.1"
  },

  "devDependencies": {
    "@types/aws-lambda": "^8.10.26",
    "@types/date-and-time": "^0.6.0",
    "@types/geojson": "^7946.0.7",
    "@types/graphql": "^14.2.0",
    "@types/lru-cache": "^5.1.0",
    "@types/node": "^11.10.4",
    "@types/node-geocoder": "^3.19.1",
    "@types/pg": "^7.4.13",
    "aws-sdk": "^2.414.0",
    "cross-env": "^5.2.0",
    "env-cmd": "^8.0.2",
    "file-loader": "^3.0.1",
    "ntl": "^3.2.4",
    "serverless": "^1.49.0",
    "serverless-dynamodb-local": "^0.2.38",
    "serverless-iam-roles-per-function": "^2.0.1",
    "serverless-offline": "^5.8.0",
    "serverless-plugin-aws-alerts": "^1.2.4",
    "serverless-webpack": "^5.3.0",
    "ts-loader": "^6.0.1",
    "ts-node": "^8.0.2",
    "typescript": "^3.3.3333",
    "webpack": "^4.32.1",
    "webpack-node-externals": "^1.7.2"
  },
  "files": [
    "dist/"
  ],
  "scripts": {
    "test-export": "env-cmd --no-override ../../config/.env cross-env-shell \"source ../../config/.env.local && ts-node --project ./tsconfig.json scripts/export\"",
    "test-geo": "env-cmd --no-override ../../config/.env cross-env-shell \"source ../../config/.env.local && ts-node --project ./tsconfig.json scripts/geocode\"",
    "deploy": "env-cmd --no-override ../../config/.env cross-env-shell \"serverless deploy --force --verbose\"",
    "package": "env-cmd --no-override ../../config/.env cross-env-shell \"serverless package --force --verbose\"",
    "remove": "env-cmd --no-override ../../config/.env cross-env-shell \"serverless remove --verbose\"",
    "dev": "env-cmd --no-override ../../config/.env cross-env-shell \"source ../../config/.env.local && serverless offline start --verbose --noTimeout --noAuth\"",
    "dynamodb": "env-cmd --no-override ../../config/.env cross-env-shell \"source ../../config/.env.local && serverless dynamodb start\"",
    "start": "ntl"
  }
}
