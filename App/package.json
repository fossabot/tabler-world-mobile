    "config:app": "env-cmd --no-override ../config/.env cross-env-shell \"source ../config/.env.local && node config/create.js\"",
    "expo": "npm run config && expo start",
    "codegen": "rm -f src/model/graphql/*.ts && npx apollo client:codegen --target=typescript --outputFlat src/model/graphql"
